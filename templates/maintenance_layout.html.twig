<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>{% block title %}Mon Prof chez moi{% endblock %}</title>

    {% block meta %}{% endblock %}

    <link rel="shortcut icon" type="image/x-icon" href="{{ absolute_url(asset('assets/images/favico_0.png')) }}">
    {% block stylesheets %}
        <link rel="stylesheet" href="{{ absolute_url(asset('assets/css/monprofchezmoi.css')) }}">
    {% endblock %}
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body class="defult-home">
<div id="loader" class="loader orange-color">
    <div class="loader-container">
        <div class='loader-icon'>
            <img src="{{ absolute_url(asset('assets/images/pre-logo.jpg')) }}" alt="">
        </div>
    </div>
</div>
<div class="main-content">

    {% block menu %}
        <div class="full-width-header home8-style4 main-home">

        </div>
    {% endblock %}
    {% block body %}{% endblock %}

</div>


<div id="scrollUp" class="orange-color">
    <i class="fa fa-angle-up"></i>
</div>

{% block javascripts %}
    <script src="{{ absolute_url(asset('assets/js/modernizr-2.8.3.min.js')) }}"></script>
    <script src="{{ absolute_url(asset('assets/js/jquery.min.js')) }}"></script>
    <script src="{{ absolute_url(asset('assets/js/bootstrap.min.js')) }}"></script>
    <script src="{{ absolute_url(asset('assets/js/rsmenu-main.js')) }}"></script>
    <script src="{{ absolute_url(asset('assets/js/jquery.nav.js')) }}"></script>
    <script src="{{ absolute_url(asset('assets/js/owl.carousel.min.js')) }}"></script>
    <script src="{{ absolute_url(asset('assets/js/slick.min.js')) }}"></script>
    <script src="{{ absolute_url(asset('assets/js/isotope.pkgd.min.js')) }}"></script>
    <script src="{{ absolute_url(asset('assets/js/imagesloaded.pkgd.min.js')) }}"></script>
    <script src="{{ absolute_url(asset('assets/js/wow.min.js')) }}"></script>
    <script src="{{ absolute_url(asset('assets/js/skill.bars.jquery.js')) }}"></script>
    <script src="{{ absolute_url(asset('assets/js/jquery.counterup.min.js')) }}"></script>
    <script src="{{ absolute_url(asset('assets/js/waypoints.min.js')) }}"></script>
    <script src="{{ absolute_url(asset('assets/js/jquery.magnific-popup.min.js')) }}"></script>
    <script src="{{ absolute_url(asset('assets/js/main.js')) }}"></script>
    <script async defer crossorigin="anonymous" src="https://connect.facebook.net/fr_FR/sdk.js#xfbml=1&version=v6.0"></script>
{% endblock %}
</body>
</html>
