{% extends 'base.html.twig' %}

{% block title %}Domaine{% endblock %}

{% block body %}
    <h1>Domaine</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ domaine.id }}</td>
            </tr>
            <tr>
                <th>Libelle</th>
                <td>{{ domaine.libelle }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ domaine.type }}</td>
            </tr>
            <tr>
                <th>Statut</th>
                <td>{{ domaine.statut ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ domaine.slug }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('backend_domaine_index') }}">back to list</a>

    <a href="{{ path('backend_domaine_edit', {'id': domaine.id}) }}">edit</a>

    {{ include('backend_domaine/_delete_form.html.twig') }}
{% endblock %}
